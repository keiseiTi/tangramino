{
  "name": "@tangramino/editor",
  "version": "0.1.0",
  "description": "A visual drag and drop editor",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./style": "./dist/style.css"
  },
  "type": "module",
  "files": [
    "src",
    "types"
  ],
  "scripts": {
    "build": "rm -rf dist types && rollup -c && tsc -p tsconfig.json",
    "watch": "rollup --watch -c & tsc -w -p tsconfig.json"
  },
  "keywords": [
    "schema",
    "engine",
    "editor",
    "visual"
  ],
  "author": "yupeilin <yuchacha250@gmail.com>",
  "license": "MIT ",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tangramino/engine": "workspace:*",
    "@tangramino/react": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.541.0",
    "nanoid": "^5.1.5",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "react": "^18",
    "react-dom": "^18",
    "rollup": "^4.46.0",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.9.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
